{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc build project",
            "command": "make",
            "args": ["-j", "12"],
            "options": {
                "cwd": "${workspaceRoot}"
